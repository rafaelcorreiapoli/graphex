{
  "name": "graphex",
  "version": "1.2.1",
  "main": "dist/index",
  "typings": "dist/index",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.3"
  },
  "scripts": {
    "dev": "tsc --watch",
    "commit": "git-cz",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc -p .",
    "semantic-release": "semantic-release",
    "playground": "nodemon src/playground/index.ts -x ts-node"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^1.0.1",
    "@semantic-release/git": "^3.0.1",
    "@types/graphql": "^0.12.4",
    "@types/jest": "^22.1.2",
    "@types/lodash": "^4.14.104",
    "@types/node": "^9.4.6",
    "@types/pluralize": "^0.0.28",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "graphql-yoga": "^1.4.3",
    "jest": "^22.3.0",
    "nodemon": "^1.17.1",
    "semantic-release": "^12.4.1",
    "ts-jest": "^22.0.4",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branch": "master",
    "repositoryUrl": "https://github.com/rafaelcorreiapoli/graphex",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "publish": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "dist/**/*.{js|css}",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      },
      "@semantic-release/github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rafaelcorreiapoli/graphex.git"
  },
  "dependencies": {
    "graphql": "^0.13.1",
    "graphql-date": "^1.0.3",
    "graphql-tools": "^2.21.0",
    "graphqltools": "^0.0.1",
    "lodash": "^4.17.5",
    "neo4j-driver": "^1.5.3",
    "pluralize": "^7.0.0",
    "util": "^0.10.3"
  }
}
